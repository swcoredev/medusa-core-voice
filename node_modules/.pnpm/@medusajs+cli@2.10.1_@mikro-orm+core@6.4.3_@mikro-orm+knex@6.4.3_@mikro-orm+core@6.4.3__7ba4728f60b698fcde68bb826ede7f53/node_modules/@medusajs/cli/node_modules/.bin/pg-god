#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/gennadii/projects_xai/medusa-core-voice/node_modules/.pnpm/pg-god@1.0.12/node_modules/pg-god/bin/node_modules:/Users/gennadii/projects_xai/medusa-core-voice/node_modules/.pnpm/pg-god@1.0.12/node_modules/pg-god/node_modules:/Users/gennadii/projects_xai/medusa-core-voice/node_modules/.pnpm/pg-god@1.0.12/node_modules:/Users/gennadii/projects_xai/medusa-core-voice/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/gennadii/projects_xai/medusa-core-voice/node_modules/.pnpm/pg-god@1.0.12/node_modules/pg-god/bin/node_modules:/Users/gennadii/projects_xai/medusa-core-voice/node_modules/.pnpm/pg-god@1.0.12/node_modules/pg-god/node_modules:/Users/gennadii/projects_xai/medusa-core-voice/node_modules/.pnpm/pg-god@1.0.12/node_modules:/Users/gennadii/projects_xai/medusa-core-voice/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../pg-god@1.0.12/node_modules/pg-god/bin/run" "$@"
else
  exec node  "$basedir/../../../../../../pg-god@1.0.12/node_modules/pg-god/bin/run" "$@"
fi
