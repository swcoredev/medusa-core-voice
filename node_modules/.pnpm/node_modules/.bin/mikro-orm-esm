#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/gennadii/projects_xai/medusa-core-voice/node_modules/.pnpm/@mikro-orm+cli@6.4.3_pg@8.16.3/node_modules/@mikro-orm/cli/node_modules:/Users/gennadii/projects_xai/medusa-core-voice/node_modules/.pnpm/@mikro-orm+cli@6.4.3_pg@8.16.3/node_modules/@mikro-orm/node_modules:/Users/gennadii/projects_xai/medusa-core-voice/node_modules/.pnpm/@mikro-orm+cli@6.4.3_pg@8.16.3/node_modules:/Users/gennadii/projects_xai/medusa-core-voice/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/gennadii/projects_xai/medusa-core-voice/node_modules/.pnpm/@mikro-orm+cli@6.4.3_pg@8.16.3/node_modules/@mikro-orm/cli/node_modules:/Users/gennadii/projects_xai/medusa-core-voice/node_modules/.pnpm/@mikro-orm+cli@6.4.3_pg@8.16.3/node_modules/@mikro-orm/node_modules:/Users/gennadii/projects_xai/medusa-core-voice/node_modules/.pnpm/@mikro-orm+cli@6.4.3_pg@8.16.3/node_modules:/Users/gennadii/projects_xai/medusa-core-voice/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  --loader ts-node/esm --no-warnings=ExperimentalWarning "$basedir/../@mikro-orm/cli/esm" "$@"
else
  exec node  --loader ts-node/esm --no-warnings=ExperimentalWarning "$basedir/../@mikro-orm/cli/esm" "$@"
fi
