#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/gennadii/projects_xai/medusa-core-voice/node_modules/.pnpm/@medusajs+cli@2.10.1_@mikro-orm+core@6.4.3_@mikro-orm+knex@6.4.3_@mikro-orm+core@6.4.3__7ba4728f60b698fcde68bb826ede7f53/node_modules/@medusajs/cli/node_modules:/Users/gennadii/projects_xai/medusa-core-voice/node_modules/.pnpm/@medusajs+cli@2.10.1_@mikro-orm+core@6.4.3_@mikro-orm+knex@6.4.3_@mikro-orm+core@6.4.3__7ba4728f60b698fcde68bb826ede7f53/node_modules/@medusajs/node_modules:/Users/gennadii/projects_xai/medusa-core-voice/node_modules/.pnpm/@medusajs+cli@2.10.1_@mikro-orm+core@6.4.3_@mikro-orm+knex@6.4.3_@mikro-orm+core@6.4.3__7ba4728f60b698fcde68bb826ede7f53/node_modules:/Users/gennadii/projects_xai/medusa-core-voice/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/gennadii/projects_xai/medusa-core-voice/node_modules/.pnpm/@medusajs+cli@2.10.1_@mikro-orm+core@6.4.3_@mikro-orm+knex@6.4.3_@mikro-orm+core@6.4.3__7ba4728f60b698fcde68bb826ede7f53/node_modules/@medusajs/cli/node_modules:/Users/gennadii/projects_xai/medusa-core-voice/node_modules/.pnpm/@medusajs+cli@2.10.1_@mikro-orm+core@6.4.3_@mikro-orm+knex@6.4.3_@mikro-orm+core@6.4.3__7ba4728f60b698fcde68bb826ede7f53/node_modules/@medusajs/node_modules:/Users/gennadii/projects_xai/medusa-core-voice/node_modules/.pnpm/@medusajs+cli@2.10.1_@mikro-orm+core@6.4.3_@mikro-orm+knex@6.4.3_@mikro-orm+core@6.4.3__7ba4728f60b698fcde68bb826ede7f53/node_modules:/Users/gennadii/projects_xai/medusa-core-voice/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@medusajs/cli/cli.js" "$@"
else
  exec node  "$basedir/../@medusajs/cli/cli.js" "$@"
fi
